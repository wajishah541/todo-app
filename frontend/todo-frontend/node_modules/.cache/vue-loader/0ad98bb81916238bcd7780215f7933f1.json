rt default {\r\n  data() {\r\n    // getTodo()\r\n    return{\r\n     \r\n    }\r\n  },\r\n  methods: {\r\n    addtodo() {\r\n      \r\n      this.tasks.push({\r\n        content: this.task,\r\n      })\r\n      this.task =\"\"\r\n    },\r\n   removeTodo(index) {\r\n     this.tasks.splice(index,1)\r\n    },\r\n    changeStatus(){\r\n      if(this.status == false)\r\n      {\r\n        this.status= true\r\n      }else{\r\n        this.status = false\r\n      }\r\n    },\r\n    finishedTodo(index){\r\n      if (this.status==true) this.tasks.splice(index,1)\r\n    }\r\n  }\r\n\r\n};\r\n</script>\r\n<style>\r\nh1 {\r\n  text-align: center;\r\n}\r\nform {\r\n  max-width: 420px;\r\n  margin: 30px auto;\r\n  background: white;\r\n  text-align: left;\r\n  padding: 40px;\r\n  border-radius: 40px;\r\n}\r\nlabel {\r\n  color: black;\r\n  display: inline-block;\r\n  margin: 25px 0 15px;\r\n  font-size: 0.6em;\r\n  text-transform: uppercase;\r\n  letter-spacing: 1px;\r\n  font-weight: bold;\r\n}\r\ntable {\r\n  width: 100%;\r\n  margin: 30px auto;\r\n  background: white;\r\n  text-align: left;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n}\r\ninput {\r\n  color: black;\r\n  display: block;\r\n  padding: 10px 6px;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  border: none;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\nbutton {\r\n  background-color: #4caf50;\r\n  border: none;\r\n  color: black;\r\n  padding: 10px 22px;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  font-size: 16px;\r\n  width: 90%;\r\n  margin: 10px;\r\n  font-weight: bold;\r\n}\r\n</style>\r\n"]}]}